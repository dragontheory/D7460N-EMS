@component panel-main html("/assets/activecss/components/panel-main/panel-main.html" get nocache) {}

meta-label:innerhtmlchange {
  #filterAppList list-item {
    @if inner-text("{#metaLabel}") {
      take-class: .active-app;
    }
  }
}
list-results list-item:click {
  list-details {
    render: "";
    var: currentUserID "";
  }
  list-results list-item {
    remove-class: .active;
  }
}
